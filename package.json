{
  "name": "fast-react-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "source": "src/index.js",
  "targets": {
    "index": {
      "includeNodeModules": {},
      "context": "browser",
      "distDir": "./buildDir",
      "outputFormat": "esmodule",
      "scopeHoist": false,
      "optimize": true,
      "publicUrl": "/"
    }
  },
  "scripts": {
    "start": "parcel ./public/index.html --port 3000 --open --no-source-maps",
    "build": "parcel build ./public/index.html --no-source-maps",
    "api": "mocker ./mock/mocker.js",
    "serve": "concurrently \"yarn api\" \"yarn start\""
  },
  "mocker": {
    "port": 7788
  },
  "proxy": "http://localhost:7788/",
  "devDependencies": {
    "@parcel/transformer-image": "2.0.0-beta.2",
    "concurrently": "^6.0.2",
    "mocker-api": "^2.8.3",
    "parcel": "^2.0.0-beta.2"
  },
  "dependencies": {
    "antd": "^4.15.2",
    "axios": "^0.21.1",
    "immutable": "^4.0.0-rc.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.3",
    "web-vitals": "^1.1.1"
  }
}
